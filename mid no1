#define _CRT_NO_WARNINGS
#include <stdio.h>

int main() {
    int number;

    // 사용자로부터 정수 입력 받기
    printf("값을 입력하세요 : ");
    scanf_s("%d", &number);

    // 입력된 숫자가 9의 배수인지 확인
    if (number % 9 == 0) {
        printf("9의 배수입니다.\n", number);
    }
    else {
        printf("9의 배수가 아닙니다.\n", number);
    }

    return 0;
}
